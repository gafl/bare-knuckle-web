<configuration>
  <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d [%t] %-5p %c - %m%n</pattern>
    </encoder>
  </appender>
  <root level="INFO">
    <appender-ref ref="CONSOLE"/>
  </root>

  <logger name="com.exilesoft" level="WARN" />

  <logger name="org.eclipse.jetty" level="WARN" />
</configuration>
